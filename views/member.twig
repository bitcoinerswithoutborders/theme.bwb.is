{% extends "base.twig" %}

{% block content %}

  <div class="content-wrapper">
    <article class="post-type-{{post.post_type}}" id="post-{{post.slug}}">
      <section class="article-content">
        <div class="article-body">
          <img src="{{post.thumbnail}}" alt="{{post.title}} Picture" title="{{post.title}}" />
          {{member}}
          {{post.content}}
        </div>
      </section>
    </article>
  </div> <!-- /content-wrapper -->
{% endblock %}
